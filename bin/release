#!/bin/sh

BUILD_DIR=${1:-}

if [ -d $BUILD_DIR/build/web ]; then
    cat <<EOF
---
default_process_types:
    web: /app/dart-sdk/bin/dart pub global run dhttpd --path /app/build/web/ 

EOF
else
    cat <<EOF
---
default_process_types:
    web: /app/dart-sdk/bin/dart pub global run dhttpd --path /app/

EOF
fi
